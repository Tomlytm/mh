{
  "expo": {
    "name": "my-app",
    "slug": "my-app",
    "version": "1.0.0",
    "jsEngine": "jsc",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/LRMSplashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "updates": {
      "enabled": false
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "enableProguardInReleaseBuilds": false,
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/market-hub-adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.markethub.lrm",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-secure-store"],
    "extra": {
      "eas": {
        "projectId": "5a732f3d-e702-40f8-87fd-392250261155"
      }
    }
  }
}
