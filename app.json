{
  "expo": {
    "name": "my-app",
    "slug": "my-app",
    "version": "1.0.0",
    "jsEngine": "jsc",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/LRMSplashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#E60000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "updates": {
      "enabled": false
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
   
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/market-hub-adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.markethub.lrm",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "ea3b119e-5a39-45e8-9ef9-fc72b2e0cf63"
      }
    },
    "owner": "lekan4real07"
  }
}
